    "def pn(a) :\n",
    "    cnt = 0\n",
    "    # 소수로만 되어있는 경우\n",
    "    if int(a) == 0:\n",
    "        b = a\n",
    "        while b != 0.0 :\n",
    "            if b != 0.0 :\n",
    "                cnt+=1\n",
    "            b = b*10\n",
    "            #소수부분 구하기\n",
    "            l = len(str(int(b)))\n",
    "            b = float('0'+str(b)[l:])\n",
    "    # 정수부분이 있는 경우\n",
    "    b = int(a)\n",
    "    while b == int(b):\n",
    "        b = b/10\n",
    "        if int(b):\n",
    "            cnt-=1\n",
    "    return cnt"
   ]
